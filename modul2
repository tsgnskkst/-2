#include <stdio.h>
int main()
{
    int k = 0;
    pos = 0;
    do
    {
        printf("Ведіть кільскість: ");
        scanf("%i", &k);
    }
    while ( k < 1 || k > 100);
    int N[k];
    for (int i = 0; i < k; i++)
    {
        scanf("%i", &N[i]);
        if ( N[i] > 0)
        {
            pos++;
        }
    }
    printf("%i\n", pos);
    if (pos!=0){
        for (int i = 0; i < k; i++)
        {
            if ( N[i] > 0)
            {
                printf("%i ", N[i]);
            }
        }
    }
    else printf("NO");
}
